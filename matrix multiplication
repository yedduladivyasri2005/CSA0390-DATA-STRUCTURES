#include<stdio.h>
#define N 50
int main()
{
	int a[N][N],b[N][N],c[N][N],m,n,p,q,i,j,k,sum=0;
	printf("enter the rows and columns of first matrix");
	scanf("%d%d",&m,&n);
	printf("enter the matrix elements");
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	printf("enter the rows and columns of second matrix");
	scanf("%d%d",&p,&q);
	printf("enter the matrix elements");
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			scanf("%d",&b[i][j]);
		}
	}
	printf("in first matrix is\n");
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
			printf("%d",a[i][j]);
		}
		printf("\n");
	}
	printf("in second matrix is\n");
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
			printf("%d",b[i][j]);
		}
		printf("\n");
	}
	if(n!=p)
	{
		printf("cannot multiply");
	}
	else
	{
		for(i=0;i<n;i++)
		{
			for(j=0;j<m;j++)
			{
				sum=0;
				for(k=0;k<m;k++)
				{
					sum=sum+(a[i][j]*b[i][j]);
				}
				c[i][j]=sum;
			}
		}
		printf("multiplication is\n");
		for(i=0;i<m;i++)
		{
			for(j=0;j<n;j++)
			{
				printf("%d\t",c[i][j]);
			}
			printf("\n");
		}
	}
	
}
